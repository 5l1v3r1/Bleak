<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netcoreapp2.2</TargetFramework>
        <PackageVersion>2.7.1</PackageVersion>
        <Authors>Akaion</Authors>
        <Description>A Windows native DLL injection library written in C# that supports several methods of injection.</Description>
        <RepositoryUrl>https://github.com/Akaion/Bleak</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <Version>2.7.0</Version>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
      <DocumentationFile>bin\Release\Bleak.xml</DocumentationFile>
    </PropertyGroup>

</Project>
